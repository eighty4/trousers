{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": false,
    "isolatedModules": true,
    "noEmit": false,
    "noEmitOnError": false
  }
}
