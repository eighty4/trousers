{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.88.0",
  "pnpmVersion": "7.20.0",
  "pnpmOptions": {
    "pnpmStore": "local",
    "strictPeerDependencies": true,
    "resolutionStrategy": "fast",
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=16",
  "suppressNodeLtsWarning": false,
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "allowMostlyStandardPackageNames": false,
  "gitPolicy": {
    "versionBumpCommitMessage": "bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "update changelogs [skip ci]"
  },
  "repository": {
    "url": "https://github.com/eighty4/trousers",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "telemetryEnabled": false,
  "hotfixChangeEnabled": false,
  "allowedProjectTags": [],
  "projects": [
    {
      "packageName": "trousers-tsconfig",
      "projectFolder": "common/tsconfig"
    },
    {
      "packageName": "example-fintech-startup",
      "projectFolder": "examples/fintech-startup"
    },
    {
      "packageName": "create-trousers-app",
      "projectFolder": "packages/create-trousers-app"
    },
    {
      "packageName": "trousers-api",
      "projectFolder": "packages/trousers-api"
    },
    {
      "packageName": "trousers-data-elasticsearch",
      "projectFolder": "packages/trousers-data-elasticsearch"
    },
    {
      "packageName": "trousers-data-interfaces",
      "projectFolder": "packages/trousers-data-interfaces"
    },
    {
      "packageName": "trousers-data-postgres",
      "projectFolder": "packages/trousers-data-postgres"
    },
    {
      "packageName": "trousers-data-sql",
      "projectFolder": "packages/trousers-data-sql"
    },
    {
      "packageName": "trousers-domain",
      "projectFolder": "packages/trousers-domain"
    },
    {
      "packageName": "trousers-service-interface",
      "projectFolder": "packages/trousers-service-interface"
    }
  ]
}
